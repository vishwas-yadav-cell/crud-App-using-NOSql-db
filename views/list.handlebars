    <table class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Mobile</th>
                <th>City</th>
            </tr>
        </thead>
        <tbody>
        {{#each list}}
        <tr>
            <td>{{this.fullName}}</td>
            <td>{{this.email}}</td>
            <td>{{this.mobile}}</td>
            <td>{{this.city}}</td>
            <td><a href="/employee/update/{{this.id}}" class="btn btn-primary">Update</a></td>
            <td><a href="/employee/delete/{{this.id}}" class="btn btn-danger">Delete</a></td>
        </tr>
        {{/each}}
        </tbody>
    </table>